[submodule "zsh/oh-my-zsh"]
	path = zsh/oh-my-zsh
	url = https://github.com/robbyrussell/oh-my-zsh.git
[submodule "nodejs/nvm"]
	path = nodejs/nvm
	url = https://github.com/creationix/nvm.git
[submodule "ruby/rbenv"]
	path = ruby/rbenv
	url = https://github.com/sstephenson/rbenv.git
	ignore = dirty
[submodule "ruby/rbenv-plugins/ruby-build"]
	path = ruby/rbenv-plugins/ruby-build
	url = https://github.com/sstephenson/ruby-build.git
[submodule "ruby/rbenv-plugins/communal-gems"]
	path = ruby/rbenv-plugins/communal-gems
	url = https://github.com/tpope/rbenv-communal-gems.git
[submodule "ruby/rbenv-plugins/default-gems"]
	path = ruby/rbenv-plugins/default-gems
	url = https://github.com/sstephenson/rbenv-default-gems.git
[submodule "tmux/plugins/tpm"]
	path = tmux/plugins/tpm
	url = https://github.com/tmux-plugins/tpm.git
[submodule "zsh/pure"]
	path = zsh/pure
	url = https://github.com/sindresorhus/pure.git
[submodule "python/pyenv"]
	path = python/pyenv
	url = https://github.com/yyuu/pyenv.git
